<XDB_ITEM type="${param.cond.itemType}" spLabel="${param.cond.fieldName}" fieldid="${param.cond.fieldId}">
	<#list param.cond.codeNameLst as emp>
		<XDB_DATA cnt="${emp?index}">
			<XDB_CODE>
				<![CDATA[${emp.code!}]]>
			</XDB_CODE>
			<XDB_NAME>
				<![CDATA[${emp.name!}]]>
			</XDB_NAME>
		</XDB_DATA>
	</#list>
</XDB_ITEM>
